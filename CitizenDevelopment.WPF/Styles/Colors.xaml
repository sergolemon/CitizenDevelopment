<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="DarkBrush" Color="#282a36" />
    <SolidColorBrush x:Key="LightDarkBrush" Color="#44475a" />
    <SolidColorBrush x:Key="BlueBrush" Color="#6272a4" />
    <SolidColorBrush x:Key="YellowBrush" Color="#f8f8f2" />

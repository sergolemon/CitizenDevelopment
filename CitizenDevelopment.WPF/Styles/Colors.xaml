    <Color x:Key="DarkColor">#282a36</Color>
    <Color x:Key="LightDarkColor">#44475a</Color>
    <Color x:Key="BlueColor">#6272a4</Color>
    <Color x:Key="YellowColor">#f8f8f2</Color>
    <Color x:Key="GreyColor">#bd93f9</Color>
    <Color x:Key="OrangeColor">#ffb86c</Color>
    <Color x:Key="RedColor">#ff5555</Color>
